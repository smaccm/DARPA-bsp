import <spi.idl4>;
import <can.idl4>;

component canbus {
    control;
    uses     can_inf   can;
    dataport Buf       spi1_client;
    dataport Buf       can_buf;

    consumes txb0_ack  txb0_ack;
    consumes txb1_ack  txb1_ack;
    consumes txb2_ack  txb2_ack;
}
